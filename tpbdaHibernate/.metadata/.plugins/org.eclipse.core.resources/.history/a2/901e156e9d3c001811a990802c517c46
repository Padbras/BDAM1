package domaine;

public class grppers {
	
	private int idgrppers;
	private groupes idgroupe;
	private personnes idpersonne;
	
	public void setIdpersonne(personnes idpersonne) {
		this.idpersonne = idpersonne;
	}
	
	public personnes getIdpersonne() {
		return idpersonne;
	}
	
	public groupes getIdgroupe() {
		return idgroupe;
	}
	
	public void setIdgroupe(groupes idgroupe) {
		this.idgroupe = idgroupe;
	}
	
	public int getIdgrppers() {
		return idgrppers;
	}
	
	public void setIdgrppers(int idgrppers) {
		this.idgrppers = idgrppers;
	}

}
